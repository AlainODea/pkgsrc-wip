FILES=\
groups.ldap.5 groups.ldap.cat5\
ldapuseradd.conf.5 ldapuseradd.conf.cat5\
users.ldap.5 users.ldap.cat5

all: $(FILES)

clean:
	-rm *.cat5

install:
	-mkdir -p @MAN_DIR@/man5
	-mkdir -p @MAN_DIR@/cat5
	cp *.5 @MAN_DIR@/man5
	cp *.cat5 @MAN_DIR@/cat5

#%.5: %.5.xml
#	xmltoman $^ > $@

%.cat5: %.5
	nroff -man < $^ > $@

